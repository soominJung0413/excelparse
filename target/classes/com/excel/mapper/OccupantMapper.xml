<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.excel.mapper.OccupantMapper">
	<resultMap type="com.excel.domain.Occupant" id="occupant">
		<id property="oNo" column="o_no" />
		<result property="oType" column="o_type" />
		<association property="monthlySalary" resultMap="com.excel.mapper.MonthlySalaryMapper.monthlySalary" />
	</resultMap>

	<select id="selectList" resultMap="occupant">
		select * from OCCUPANT where o_no > 0
	</select>

</mapper>